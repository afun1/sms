# Add these for alternative SMS providers

# TextBelt (free tier: 1 message per day, paid plans available)
SMS_API_KEY=textbelt  # Use 'textbelt' for free tier or your paid API key

# ClickSend (paid service, no A2P required for low volume)
CLICKSEND_USERNAME=your_clicksend_username
CLICKSEND_API_KEY=your_clicksend_api_key

# Sender ID (optional, for some providers)
SENDER_ID=YourApp
